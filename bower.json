{
  "name": "starter-express-gulp-requirejs",
  "version": "0.1.0",
  "homepage": "https://github.com/nathanstilwell/starter-express-gulp-requirejs",
  "authors": [
    "Nathan Stilwell <nathanstilwell@gmail.com>"
  ],
  "description": "Admin for Gilt Editorial Stories",
  "license": "WTFPL",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "install": {
    "path": {
      "js": "assets/javascript/vendor",
      "/[c|le]ss$/": "assets/less/vendor"
    },
    "sources" : {
      "modernizr" : "bower_components/modernizr/modernizr.js"
    }
  },
  "dependencies": {
    "backbone": "~1.2.1",
    "jquery": "~2.1.4",
    "requirejs": "~2.1.18",
    "normalize.css": "~3.0.3",
    "underscore": "~1.8.3",
    "modernizr": "~2.8.3"
  }
}
